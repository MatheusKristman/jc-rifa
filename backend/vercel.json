{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/index.js"
        }
    ],
    "functions": {
        "src/controllers/AccountController.js": {
            "includeFiles": "src/resources/mail/forgotPassword.html"
        }
    }
}
